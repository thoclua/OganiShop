@model CategoryNew


<div class="modal fade bd-example-nodal-lg" id="create-or-update-cate" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">CategoryProduct</h5>
                <button type="button" class="close button-close" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div>
                    <div class="form-group row">
                        <div class="form-group col-md-6 mb-3">
                            <label for="title" class="col-sm-2 col-form-label">Title</label>
                            <input type="text" class="form-control" id="title" placeholder="Title" value="@Model.CategoryName" />
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary button-close">Close</button>
                    <button type="button" class="btn btn-primary" id="btn-save-cate" sp-id="@Model.id">Save Changes</button>
                </div>
            </div>

        </div>
    </div>
</div>